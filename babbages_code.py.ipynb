    "import numpy as np\n",
    "def statistics(grades):\n",
    "    standard = np.std(grades)\n",
    "    average = np.mean(grades)\n",
    "    high = max(grades)\n",
    "    low = min(grades)\n",
    "    return average,standard,high,low"
